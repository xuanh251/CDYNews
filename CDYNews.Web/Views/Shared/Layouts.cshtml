<!DOCTYPE HTML>
<html>
<head>
    <title>Website tin tức Trường Cao đẳng Y tế Quảng Nam | Trang chủ</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href="~/Assets/client/css/bootstrap-3.1.1.min.css" rel="stylesheet" />
    <!-- Custom Theme files -->
    <link href="~/Assets/client/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/Assets/client/css/style.css" rel="stylesheet" />
    <script src="~/Assets/client/js/jquery.min.js"></script>
    <script src="~/Assets/client/js/move-top.js"></script>
    <script src="~/Assets/client/js/easing.js"></script>
    <link href="~/Assets/client/css/flexslider.css" rel="stylesheet" />
    <!--/script-->
    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 900);
            });
        });
    </script>
</head>
<body>
    @{ Html.RenderAction("Header", "Home");}
    <!--/start-banner-->
    @RenderBody()
    <!--//end-main-->
    <!--/start-footer-section-->
    @{ Html.RenderAction("Footer", "Home");}
    <!--start-smoth-scrolling-->
    <script type="text/javascript">
        $(document).ready(function () {
            /*
            var defaults = {
                    containerID: 'toTop', // fading element id
                containerHoverID: 'toTopHover', // fading element hover id
                scrollSpeed: 1200,
                easingType: 'linear'
                };
            */

            $().UItoTop({ easingType: 'easeOutQuart' });

        });
    </script>
    <a href="#home" id="toTop" class="scroll" style="display: block;"> <span id="toTopHover" style="opacity: 1;"> </span></a>


    <!--JS-->
    <script src="~/Assets/client/js/bootstrap-3.1.1.min.js"></script>

    <!--//JS-->

</body>
</html>